            android:name=".CustomPhotoGalleryActivity"
            android:label="@string/title_activity_upload__property_" />
        <activity
            android:name=".Property_Located_Activity"
            android:label="@string/title_activity_residential__commercial_">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.asad.homebuyerproject.HomeActivity" />
        </activity>
        <activity android:name=".Activity_Uplaod_Property_Part2" />
