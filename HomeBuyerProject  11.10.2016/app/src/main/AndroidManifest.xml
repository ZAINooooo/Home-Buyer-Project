                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.asad.homebuyerproject.HomeActivity" />
        </activity>
        <activity
            android:name=".LoginRegister"
            android:label="@string/title_activity_login_register" />
        <activity
            android:name=".Register"
            android:label="@string/title_activity_register" />
        <activity
            android:name=".PostProperty"
            android:label="@string/title_activity_post_property">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.asad.homebuyerproject.HomeActivity" />
        </activity>
        <activity
            android:name=".SellRentActivty"
            android:label="@string/title_activity_sell_rent_activty">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.asad.homebuyerproject.HomeActivity" />
        </activity>
        <activity
            android:name=".Residential_Commercial_Activity"
            android:label="@string/title_activity_residential__commercial_">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.asad.homebuyerproject.HomeActivity" />
        </activity>
        <activity
            android:name=".Upload_Property_Activity"
            android:label="@string/title_activity_upload__property_" />
