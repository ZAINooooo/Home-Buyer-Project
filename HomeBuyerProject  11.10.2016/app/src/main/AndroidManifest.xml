            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <activity
            android:name=".Activity_Post_property_Click"
            android:label="@string/title_activity__post_property__click">

            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.asad.homebuyerproject.HomeActivity" />
